n      interventionApi.countIntervention().then(data => this.length = data)\r\n    }\r\n  },\r\n  computed: {\r\n    nbPageComputed() {\r\n      return this.pageCount;\r\n    },\r\n    key: {\r\n      get: function() {\r\n        return this.keyword;\r\n      },\r\n      set: function(keyword) {\r\n        this.keyword = keyword;\r\n      },\r\n    },\r\n\r\n  },\r\n};\r\n</script>\r\n<style scoped></style>\r\n"]}]}