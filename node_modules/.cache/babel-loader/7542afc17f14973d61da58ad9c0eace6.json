 'axios';\r\nimport { requestOptions } from '@/_helpers/request-options.js';\r\n\r\nexport const noteApi = {\r\n    getById,\r\n    getAllByPage,\r\n    getCount,\r\n    save,\r\n    deleteNote,\r\n\r\n    getAllByIdEtudiant,\r\n    getAllNotes\r\n}\r\n\r\nconst END_POINT = \"notes\";\r\n\r\nfunction getById(id){\r\n    let req = `/notes/${id}`;\r\n\r\n    return  axios\r\n        .get(req, requestOptions.headers())\r\n        .then(response => response.data)\r\n        .catch((error) => console.log(error));\r\n}\r\n\r\nfunction getAllByPage(page, size, search = \"\"){\r\n    let req = `/notes/${page}/${size}/${search}`;\r\n\r\n    return  axios\r\n        .get(req, requestOptions.headers())\r\n        .then(response => response.data)\r\n        .catch((error) => console.log(error));\r\n}\r\n\r\nfunction getCount(search = \"\"){\r\n    let req = `/notes/count/${search}`;\r\n\r\n    return  axios\r\n        .get(req, requestOptions.headers())\r\n        .then(response => response.data[\"nb\"])\r\n        .catch((error) => console.log(error));\r\n}\r\n\r\nfunction save(form) {\r\n    let req =  \"notes\";\r\n\r\n    return axios\r\n        .post(req, form, requestOptions.headers())\r\n        .then((response) => response)\r\n        .catch((error) => console.log(error));\r\n\r\n}\r\n\r\nfunction deleteNote(id) {\r\n    let req =  `notes/${id}`;\r\n\r\n    return axios\r\n        .delete(req, requestOptions.headers())\r\n        .then((response) => response)\r\n        .catch((error) => console.log(error));\r\n\r\n}\r\n\r\nfunction getAllByIdEtudiant(id) {\r\n    let req =  `notes/etudiant/${id}`;\r\n\r\n    return axios\r\n        .get(req, requestOptions.headers())\r\n        .then((response) => response.data)\r\n        .catch((error) => console.log(error));\r\n\r\n}\r\n\r\nfunction getAllNotes() {\r\n    return axios\r\n      .get(`${END_POINT}`, requestOptions.headers())\r\n      .then((response) => response.data)\r\n      .catch((error) => console.log(error));\r\n  }"]}]}