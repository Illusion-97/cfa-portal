,EACA,KAAA,UAAA,aAAA,KAAA,QAAA,CAAA,KAAA;AACA;AAJA,GAZA;AAkBA,EAAA,IAlBA,kBAkBA;AACA,WAAA;AACA,MAAA,KAAA,EAAA,EADA;AAEA,MAAA,OAAA,EAAA,EAFA;AAGA,MAAA,SAAA,EAAA,CAHA;AAKA,MAAA,MAAA,EAAA,EALA;AAOA,MAAA,UAAA,EAAA;AAPA,KAAA;AASA,GA5BA;AA6BA,EAAA,QAAA,EAAA;AACA,IAAA,aADA,2BACA;AACA,aAAA,KAAA,KAAA;AACA,KAHA;AAIA,IAAA,cAJA,4BAIA;AACA,aAAA,KAAA,SAAA;AACA;AANA,GA7BA;AAqCA,EAAA,OArCA,qBAqCA;AACA,SAAA,WAAA;AACA,GAvCA;AAwCA,EAAA,OAAA,EAAA;AACA,IAAA,MADA,kBACA,CADA,EACA;AAAA;;AACA,MAAA,CAAA,CAAA,cAAA;AACA,MAAA,OAAA,CACA,YADA,CACA,CADA,EACA,KAAA,OADA,EACA,KAAA,MADA,EAEA,IAFA,CAEA,UAAA,QAAA;AAAA,eAAA,KAAA,CAAA,KAAA,GAAA,QAAA;AAAA,OAFA;AAGA,MAAA,OAAA,CACA,QADA,CACA,KAAA,MADA,EAEA,IAFA,CAGA,UAAA,QAAA;AAAA,eAAA,KAAA,CAAA,SAAA,GAAA,IAAA,CAAA,IAAA,CAAA,QAAA,GAAA,KAAA,CAAA,OAAA,CAAA;AAAA,OAHA;AAKA,KAXA;AAYA,IAAA,UAZA,sBAYA,OAZA,EAYA;AAAA;;AACA,MAAA,OAAA,CACA,YADA,CACA,OAAA,GAAA,CADA,EACA,KAAA,OADA,EAEA,IAFA,CAEA,UAAA,QAAA;AAAA,eAAA,MAAA,CAAA,KAAA,GAAA,QAAA;AAAA,OAFA;AAGA,KAhBA;AAiBA,IAAA,WAjBA,yBAiBA;AAAA;;AACA,MAAA,OAAA,CACA,YADA,CACA,CADA,EACA,KAAA,OADA,EAEA,IAFA,CAEA,UAAA,QAAA;AAAA,eAAA,MAAA,CAAA,KAAA,GAAA,QAAA;AAAA,OAFA;AAGA,MAAA,OAAA,CACA,QADA,GAEA,IAFA,CAGA,UAAA,QAAA;AAAA,eAAA,MAAA,CAAA,SAAA,GAAA,IAAA,CAAA,IAAA,CAAA,QAAA,GAAA,MAAA,CAAA,OAAA,CAAA;AAAA,OAHA;AAKA,KA1BA;AA2BA,IAAA,UA3BA,sBA2BA,MA3BA,EA2BA;AAAA;;AACA,MAAA,OAAA,CAAA,UAAA,CAAA,MAAA,EAAA,IAAA,CAAA;AAAA,eAAA,MAAA,CAAA,WAAA,EAAA;AAAA,OAAA;AACA,KA7BA;AA8BA,IAAA,SA9BA,qBA8BA,IA9BA,EA8BA;AACA,WAAA,UAAA,GAAA,IAAA,CAAA,MAAA;AACA,WAAA,KAAA,CAAA,YAAA,EAAA,IAAA;AACA,KAjCA;AAkCA,IAAA,QAlCA,oBAkCA,IAlCA,EAkCA;AACA,UAAA,KAAA,GAAA,KAAA,MAAA,CAAA,IAAA,CAAA,KAAA,CAAA,GAAA,EAAA,MAAA,CAAA,CAAA,CAAA;AAEA,UAAA,KAAA,CAAA,CAAA,CAAA,IAAA,OAAA,EAAA,KAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,mBAAA;AAAA,QAAA,MAAA,EAAA;AAAA,UAAA,EAAA,EAAA,IAAA,CAAA;AAAA;AAAA,OAAA,EAAA,KACA,IAAA,KAAA,CAAA,CAAA,CAAA,IAAA,UAAA,EAAA,KAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,sBAAA;AAAA,QAAA,MAAA,EAAA;AAAA,UAAA,EAAA,EAAA,IAAA,CAAA;AAAA;AAAA,OAAA,EAAA,KACA,IAAA,KAAA,CAAA,CAAA,CAAA,IAAA,WAAA,EAAA,KAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,uBAAA;AAAA,QAAA,MAAA,EAAA;AAAA,UAAA,EAAA,EAAA,IAAA,CAAA;AAAA;AAAA,OAAA,EALA,CAMA;AACA;AACA;AA1CA;AAxCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"container-fluid\">\r\n    <div class=\"header-list\">\r\n      <div class=\"text-align-left\" id=\"groupe-input\" v-if=\"!isAction\">\r\n        <label class=\"col-1\">note</label>\r\n        <input class=\"col-9 form-control\" type=\"text\" :value=\"note_input\" disabled=\"disabled\"/>\r\n      </div>\r\n\r\n      <form class=\"form-inline form\" @submit=\"submit\">\r\n        <input\r\n          id=\"saisie\"\r\n          name=\"saisie\"\r\n          type=\"text\"\r\n          class=\"form-control\"\r\n          v-model=\"saisie\"\r\n        />\r\n        <button class=\"btn-submit\" type=\"submit\">\r\n          <font-awesome-icon :icon=\"['fas', 'search']\" class=\"icon\"/>\r\n        </button>\r\n      </form>\r\n\r\n      <router-link class=\"btn btn-info\" :to=\"{ name: 'admin_note_create' }\" v-if=\"isAction\"\r\n        >Ajouter</router-link\r\n      >\r\n    </div>\r\n    <table class=\"table table-bordered table-striped table-hover\">\r\n      <thead class=\"thead-dark\">\r\n        <tr>\r\n          <th>Etudiant</th>\r\n          <th>Devoir/Examen</th>\r\n          <th>Note</th>\r\n          <th>Observations</th>\r\n          <!-- <th v-if=\"isAction\">Actions</th> -->\r\n        </tr>\r\n      </thead>\r\n      <tbody v-if=\"notesComputed\">\r\n        <tr v-for=\"note in notesComputed\" :key=\"note.id\" class=\"mon-tr\"\r\n          v-on:click=\"clickList(note)\"\r\n          @dblclick=\"dblClick(note)\">\r\n          <td>{{ note.etudiantDto.prenom }} {{ note.etudiantDto.nom }}</td>\r\n          <td> \r\n              <span v-if=\"note.devoirDto\">{{ note.devoirDto.enonce }}</span>\r\n              <span v-if=\"note.examenDto\">{{ note.examenDto.enonce }}</span>\r\n          </td>\r\n          <td>{{ note.noteObtenu }}</td>\r\n          <td>{{ note.observations }}</td>\r\n          <!-- <td v-if=\"isAction\">\r\n            <router-link\r\n              class=\"btn btn-info\"\r\n              :to=\"{ name: 'admin_note_detail', params: { id: note.id } }\"\r\n              >Detail</router-link\r\n            >\r\n            &nbsp;\r\n            <router-link\r\n              class=\"btn btn-success\"\r\n              :to=\"{ name: 'admin_note_update', params: { id: note.id } }\"\r\n              >Update</router-link\r\n            >\r\n            &nbsp;\r\n            <button class=\"btn btn-danger\" v-on:click=\"deleteNote(note.id)\">\r\n              Supprimer\r\n            </button>\r\n          </td> -->\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <paginate\r\n      :page-count=\"pageCount\"\r\n      :page-range=\"1\"\r\n      :margin-pages=\"2\"\r\n      :click-handler=\"pageChange\"\r\n      :prev-text=\"'Prev'\"\r\n      :next-text=\"'Next'\"\r\n      :container-class=\"'pagination'\"\r\n      :page-class=\"'page-item'\"\r\n      :page-link-class=\"'page-link'\"\r\n      :prev-class=\"'page-item'\"\r\n      :next-class=\"'page-item'\"\r\n      :prev-link-class=\"'page-link'\"\r\n      :next-link-class=\"'page-link'\"\r\n      :active-class=\"'active'\"\r\n    >\r\n      >\r\n    </paginate>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { noteApi } from \"@/_api/note.api.js\";\r\n\r\nexport default {\r\n  name: \"noteListeComponent\",\r\n  components: {},\r\n  props: {\r\n    isAction: {\r\n      type: Boolean,\r\n      default: false,\r\n    },\r\n    noteProp: {\r\n      default: null,\r\n    }\r\n  },\r\n  watch: {\r\n    noteProp(){\r\n      if (this.noteProp != null) \r\n        this.note_input = `${this.noteProp.titre}`;\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      notes: [],\r\n      perPage: 10,\r\n      pageCount: 0,\r\n\r\n      saisie: \"\",\r\n\r\n      note_input: \"\",\r\n    };\r\n  },\r\n  computed: {\r\n    notesComputed() {\r\n      return this.notes;\r\n    },\r\n    nbPageComputed() {\r\n      return this.pageCount;\r\n    },\r\n  },\r\n  created() {\r\n    this.refreshList();\r\n  },\r\n  methods: {\r\n    submit(e) {\r\n      e.preventDefault();\r\n      noteApi\r\n        .getAllByPage(0, this.perPage, this.saisie)\r\n        .then((response) => (this.notes = response));\r\n      noteApi\r\n        .getCount(this.saisie)\r\n        .then(\r\n          (response) => (this.pageCount = Math.ceil(response / this.perPage))\r\n        );\r\n    },\r\n    pageChange(pageNum) {\r\n      noteApi\r\n        .getAllByPage(pageNum - 1, this.perPage)\r\n        .then((response) => (this.notes = response));\r\n    },\r\n    refreshList() {\r\n      noteApi\r\n        .getAllByPage(0, this.perPage)\r\n        .then((response) => (this.notes = response));\r\n      noteApi\r\n        .getCount()\r\n        .then(\r\n          (response) => (this.pageCount = Math.ceil(response / this.perPage))\r\n        );\r\n    },\r\n    deleteNote(noteId) {\r\n      noteApi.deleteNote(noteId).then(() => this.refreshList());\r\n    },\r\n    clickList(note) {\r\n      this.note_input = note.enonce;\r\n      this.$emit('click-list',note);\r\n    },\r\n    dblClick(note){\r\n      let route = this.$route.path.split(\"/\").splice(1);\r\n\r\n      if(route[0]== 'admin') this.$router.push({name:'admin_note_detail', params: { id: note.id }}); \r\n      else if(route[0]== 'referent')  this.$router.push({name:'referent_note_detail', params: { id: note.id }});\r\n      else if(route[0]== 'formateur') this.$router.push({name:'formateur_note_detail', params: { id: note.id }});\r\n      // else if(route[0]== 'cef') this.$router.push({name:'cef_note_detail', params: { id: note.id }});\r\n      // else if(route[0]== 'etudiant') this.$router.push({name:'etudiant_note_detail', params: { id: note.id }});\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped src=\"@/assets/styles/CrudListComponent.css\">\r\n</style>\r\n"],"sourceRoot":"src/components/List"}]}