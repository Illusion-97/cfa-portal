mplate,\r\n  },\r\n  data() {\r\n    return {\r\n      currentPage: 1,\r\n      perPage: 10,\r\n      pageCount: 0,\r\n      keyword: \"\",\r\n      items: [],\r\n      fields: absences_latesFields,\r\n    };\r\n  },\r\n  created() {\r\n    this.fillList();\r\n  },\r\n  methods: {\r\n    fillList() {\r\n      absencesApi\r\n        .getAllAbsences(this.currentPage, this.perPage, this.key)\r\n        .then((data) => (this.items = data));\r\n      this.countAbsence();\r\n    },\r\n    countAbsence() {\r\n      absencesApi\r\n        .countAbsence(this.key)\r\n        .then((data) => (this.pageCount = Math.ceil(data / this.perPage)));\r\n    },\r\n    pageChange(page) {\r\n      absencesApi\r\n        .getAllAbsences(page, this.perPage, this.key)\r\n        .then((data) => (this.items = data));\r\n      this.countAbsence();\r\n    },\r\n    search(evt) {\r\n      evt.preventDefault();\r\n      this.fillList();\r\n      this.countAbsence();\r\n    },\r\n  },\r\n  computed: {\r\n    nbPageComputed() {\r\n      return this.pageCount;\r\n    },\r\n    key: {\r\n      get: function() {\r\n        return this.keyword;\r\n      },\r\n      set: function(keyword) {\r\n        this.keyword = keyword;\r\n      },\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.opts,\r\nlabel {\r\n  color: black;\r\n}\r\n\r\n.table {\r\n  text-align: center;\r\n}\r\n\r\n.icon:hover {\r\n  cursor: pointer;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/Referent"}]}