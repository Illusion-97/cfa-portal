A,IAAA,MADA,oBACA;AACA,WAAA,OAAA,CAAA,EAAA,CAAA,CAAA,CAAA;AACA,KAHA;AAIA,IAAA,QAJA,oBAIA,KAJA,EAIA;AAAA;;AACA,MAAA,KAAA,CAAA,cAAA,GADA,CAEA;;AACA,MAAA,YAAA,CACA,eADA,CACA,KAAA,IADA,EAEA,IAFA,CAEA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,CAAA,MAAA,KAAA,GAAA,EACA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA;AACA,OALA;AAMA,KAbA;AAcA,IAAA,OAdA,mBAcA,KAdA,EAcA;AAAA;;AACA,MAAA,KAAA,CAAA,cAAA,GADA,CAEA;;AACA,WAAA,IAAA,CAAA,KAAA,GAAA,EAAA;AACA,WAAA,IAAA,CAAA,OAAA,GAAA,EAAA,CAJA,CAKA;AACA;;AACA,WAAA,IAAA,GAAA,KAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,IAAA;AACA,OAFA;AAGA;AAzBA,GAfA;AA0CA,EAAA,OA1CA,qBA0CA;AAAA;;AACA,QAAA,KAAA,WAAA,IAAA,IAAA,IAAA,KAAA,WAAA,IAAA,EAAA,IAAA,KAAA,WAAA,IAAA,CAAA,EAAA;AACA,MAAA,YAAA,CACA,gBADA,CACA,KAAA,WADA,EAEA,IAFA,CAEA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,IAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,MAAA,CAAA,KAAA;AACA,OALA;AAMA;AACA;AAnDA,CAAA","sourcesContent":["<template>\r\n    <section>\r\n        <!-- TODO : changer le chemin de retour en fonction du role. Si ADMIN => lst ADMIN sinn Si REF => lst REF -->\r\n        <router-link :to=\"{ name: 'admin_formation_list' }\" class=\"h5\"\r\n            style=\"cursor:pointer; color:black;text-decoration:none;\">\r\n            <font-awesome-icon :icon=\"['fas', 'chevron-left']\" class=\"icon\" />\r\n            Precedent\r\n        </router-link>\r\n        <h1 class=\"text-center\">Ajouter une formation</h1>\r\n        <div class=\"container\">\r\n            <div class=\"row\">\r\n                <div class=\" col-md-12 div-form\">\r\n                    <b-form @submit=\"onSubmit\" @reset=\"onReset\" v-if=\"show\">\r\n                        <b-form-group label=\"Nom de la formation :\" label-for=\"formation\">\r\n                            <b-form-input v-model=\"form.titre\" id=\"formation\" class=\"form-control\"\r\n                                placeholder=\"Ex : Java Init., Photoshop Init.\">\r\n                            </b-form-input>\r\n                        </b-form-group>\r\n\r\n                        <b-form-group label=\"Description :\">\r\n                            <b-form-textarea v-model=\"form.contenu\" rows=\"3\" max-rows=\"6\"\r\n                                placeholder=\"Description de la formation\" id=\"desc\">\r\n                            </b-form-textarea>\r\n                        </b-form-group>\r\n\r\n                        <div class=\"d-flex justify-content-between\" v-if=\"formationId == null\">\r\n                            <b-button type=\"reset\" variant=\"outline-danger\">Annuler</b-button>\r\n                            <b-button type=\"submit\" variant=\"outline-info\" class=\" px-3\">Envoyer</b-button>\r\n                        </div>\r\n                        <div class=\"d-flex justify-content-end\" v-else>\r\n                            <b-button type=\"submit\" variant=\"outline-info\" class=\" px-3\">Envoyer</b-button>\r\n                        </div>\r\n                    </b-form>\r\n\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </section>\r\n</template>\r\n\r\n<script>\r\n    import { formationApi } from \"@/_api/formation.api.js\";\r\n    export default {\r\n        name: \"formation-form\",\r\n        components: {},\r\n        data() {\r\n            return {\r\n                form: {\r\n                    id: \"\",\r\n                    titre: \"\",\r\n                    contenu: \"\",\r\n                },\r\n                items: {},\r\n                formationId: this.$route.params.id,\r\n                show: true,\r\n            };\r\n        },\r\n        methods: {\r\n            goBack() {\r\n                this.$router.go(-1);\r\n            },\r\n            onSubmit(event) {\r\n                event.preventDefault();\r\n                // a modifier : ajout pour l'admin et ajout pour le referent\r\n                formationApi\r\n                    .insertFormation(this.form)\r\n                    .then((data) => {\r\n                        if (data.status === 200)\r\n                            this.$router.push({ name: \"admin_formation_list\" });\r\n                    });\r\n            },\r\n            onReset(event) {\r\n                event.preventDefault();\r\n                // Reset our form values\r\n                this.form.titre = \"\"\r\n                this.form.contenu = \"\";\r\n                // this.form.cursusLstDto = [];\r\n                // Trick to reset/clear native browser form validation state\r\n                this.show = false;\r\n                this.$nextTick(() => {\r\n                    this.show = true;\r\n                });\r\n            },\r\n        },\r\n        created() {\r\n            if (this.formationId != null && this.formationId != \"\" && this.formationId != 0) {\r\n                formationApi\r\n                    .getFormationById(this.formationId)\r\n                    .then((data) => {\r\n                        this.items = data;\r\n                        this.form = this.items;\r\n                    });\r\n            }\r\n        },\r\n    };\r\n</script>\r\n\r\n<style scoped>\r\n    /* .div-form {\r\n    text-align: left;\r\n  } */\r\n    #formation,\r\n    #desc,\r\n    #cursus {\r\n        width: 100%;\r\n        margin-bottom: 1em;\r\n        height: auto;\r\n    }\r\n\r\n    #formation,\r\n    #cursus,\r\n    .form-control {\r\n        border: 1px solid #767676;\r\n        border-radius: 0.2em;\r\n    }\r\n\r\n    .form-select {\r\n        /* width: 100%; */\r\n        height: calc(1.5em + 0.5rem + 2px);\r\n        border-radius: 0.2em;\r\n    }\r\n\r\n    .select-title {\r\n        background-color: #76767625;\r\n    }\r\n</style>"],"sourceRoot":"src/views/All/Formation"}]}