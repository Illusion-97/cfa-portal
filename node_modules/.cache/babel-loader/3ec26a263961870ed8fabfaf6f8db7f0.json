AAIA,IAAA,UAJA,wBAIA;AACA,UAAA,KAAA,GAAA,KAAA,MAAA,CAAA,IAAA,CAAA,KAAA,CAAA,GAAA,EAAA,MAAA,CAAA,CAAA,CAAA;;AACA,UAAA,KAAA,CAAA,CAAA,CAAA,IAAA,OAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AACA,UAAA,IAAA,EAAA;AADA,SAAA;AAIA,OALA,MAMA,IAAA,KAAA,CAAA,CAAA,CAAA,IAAA,UAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AACA,UAAA,IAAA,EAAA;AADA,SAAA;AAIA,OALA,MAMA,IAAA,KAAA,CAAA,CAAA,CAAA,IAAA,KAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AACA,UAAA,IAAA,EAAA;AADA,SAAA;AAIA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AACA;AAnCA,GAVA;AA+CA,EAAA,OA/CA,qBA+CA;AAAA;;AACA,IAAA,OAAA,CACA,OADA,CACA,KAAA,MAAA,CAAA,MAAA,CAAA,EADA,EAEA,IAFA,CAEA,UAAA,QAAA;AAAA,aAAA,KAAA,CAAA,IAAA,GAAA,QAAA;AAAA,KAFA;AAGA;AAnDA,CAAA","sourcesContent":["<template>\r\n  <div class=\"container-fluid\">\r\n    <a\r\n      @click=\"goBack()\"\r\n      class=\"h5\"\r\n      style=\"cursor:pointer; color:black;text-decoration:none;\"\r\n    >\r\n      <font-awesome-icon :icon=\"['fas', 'chevron-left']\" class=\"icon\" />\r\n      Precedent\r\n    </a>\r\n\r\n    <b-card no-body id=\"my-card\">\r\n      <b-card-header>\r\n        <span class=\"\">Detail</span>\r\n        <button class=\"btn btn-info\" v-on:click=updateNote>\r\n              Update \r\n            </button>\r\n      </b-card-header>\r\n\r\n      <b-card-text class=\"identity row ml-5\">\r\n        <span class=\"font-weight-bold col-md-2\">Etudiant :</span>\r\n        <span class=\"col-md-4\">{{ note.etudiantDto.utilisateurDto.prenom }} {{ note.etudiantDto.utilisateurDto.nom }}</span>\r\n      </b-card-text>\r\n\r\n      <b-card-text class=\"identity row ml-5\">\r\n        <span class=\"font-weight-bold col-md-2\">Devoir/Examens :</span>\r\n        <span class=\"col-md-4\">\r\n          <span v-if=\"note.devoirDto\">{{ note.devoirDto.enonce }}</span\r\n          ><span v-if=\"note.examenDto\">{{\r\n            note.examenDto.examenDto.enonce\r\n          }}</span></span\r\n        >\r\n      </b-card-text>\r\n\r\n      <b-card-text class=\"identity row ml-5\">\r\n        <span class=\"font-weight-bold col-md-2\">Note :</span>\r\n        <span class=\"col-md-4\"> {{ note.noteObtenu }}</span>\r\n      </b-card-text>\r\n\r\n      <b-card-text class=\"identity row ml-5\">\r\n        <span class=\"font-weight-bold col-md-2\">Observations :</span>\r\n        <span class=\"col-md-4\">{{ note.observations }}</span>\r\n      </b-card-text>\r\n    </b-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { noteApi } from \"@/_api/note.api.js\";\r\nexport default {\r\n  name: \"NoteDetail\",\r\n  components: {},\r\n  data() {\r\n    return {\r\n      noteId: this.$route.params.id,\r\n      note: {etudiantDto: {}},\r\n      loading: false,\r\n    };\r\n  },\r\n  methods: {\r\n    goBack() {\r\n      this.$router.go(-1);\r\n    },\r\n    updateNote(){\r\n      let route = this.$route.path.split(\"/\").splice(1);\r\n      if(route[0]== 'admin'){\r\n      this.$router.push({\r\n        name: \"admin_note_update\",\r\n        \r\n      });\r\n      }\r\n      else if (route[0] == 'referent') {\r\n        this.$router.push({\r\n        name: \"referent_note_update\",\r\n        \r\n      });\r\n      }\r\n      else if (route[0] == 'cef') {\r\n        this.$router.push({\r\n        name: \"cef_note_update\",\r\n        \r\n      });\r\n      }\r\n      /*else {\r\n        this.$router.push({\r\n        name: \"formateur_note_update\",\r\n        \r\n      });\r\n      }\r\n      else {\r\n        this.$router.push({\r\n        name: \"cef_note_update\",\r\n      });\r\n      }*/\r\n    },\r\n  },\r\n  created() {\r\n    noteApi\r\n      .getById(this.$route.params.id)\r\n      .then((response) => (this.note = response));\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped src=\"@/assets/styles/CrudDetail.css\"></style>\r\n"],"sourceRoot":"src/views/Admin/Crud/Note"}]}